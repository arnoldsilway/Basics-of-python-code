from array import
  x=array('i',[])
  n=int(input("Enter number of elements:"))
  for i in range(n):
     x.append (int(input("Enter the number:")))
     print(x)
  key=int(input("Enter the element you want to search:"))
  for i in range(n):
      if x[i]==key:
         print("the number if formed in location")
         print(i)
